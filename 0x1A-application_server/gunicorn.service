[Unit]
Description=Gunicorn instance to serve application
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app --error-logfile /tmp/Airbnb-4-error.log
Restart=always
StandardOutput=append:/tmp/Airbnb-4-access.log
StandardError=append:/tmp/Airbnb-4-error.log
SyslogIdentifier=gunicorn

[Install]
WantedBy=multi-user.target
